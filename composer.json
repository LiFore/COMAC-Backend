{
    "name": "caoym/phpboot-example",
    "description": "Examples of using phpboot",
    "keywords": [
        "restful",
        "dependency-injection",
        "orm",
        "swagger",
        "workflow"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "caoym/phpboot": "dev-master",
        "php-curl-class/php-curl-class": "^8.8",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "voku/simple_html_dom": "^4.7",
        "voku/portable-utf8": "^5.4",
        "ext-gd": "*",
        "qiniu/php-sdk": "^7.4",
      "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "authors": [
        {
            "name": "Yangmin Cao",
            "email": "caoyangmin@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "App/",
            "App\\Controllers\\innerController\\": "App/Controllers/innerController/",
            "App\\Controllers\\outerController\\": "App/Controllers/outerController/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "classmap": [

        ]
    }
}
